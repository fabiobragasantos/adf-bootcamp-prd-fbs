{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-bootcamp-prd-fbs"
		},
		"ls_AzureSql_bootcamp_prd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSql_bootcamp_prd'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sql-bootcamp-prd-fbs.database.windows.net;Initial Catalog=sqldb-bootcamp-prd-fbs;User ID=usr-bootcamp-prd-fbs"
		},
		"ls_dls_bootcamp_prd_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dls_bootcamp_prd'"
		},
		"ls_Http_Api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummyjson.com/users"
		},
		"ls_Webtable_bootcamp_prd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.dieese.org.br/analisecestabasica/salarioMinimo.html"
		},
		"ls_adb_bootcamp_prd_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0507-192430-gzco5d8r"
		},
		"ls_dls_bootcamp_prd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsbootcampprdfbs.dfs.core.windows.net/"
		},
		"ls_kv_bootcamp_prd_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-bootcamp-prd-fbs.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-bootcamp-prd-fbs_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dls_bootcamp_prd_accountKey": {
						"value": "[parameters('ls_dls_bootcamp_prd_accountKey')]"
					},
					"ls_AzureSql_bootcamp_prd_connectionString": {
						"value": "[parameters('ls_AzureSql_bootcamp_prd_connectionString')]"
					},
					"ls_Http_Api_properties_typeProperties_url": {
						"value": "[parameters('ls_Http_Api_properties_typeProperties_url')]"
					},
					"ls_dls_bootcamp_prd_properties_typeProperties_url": {
						"value": "[parameters('ls_dls_bootcamp_prd_properties_typeProperties_url')]"
					},
					"ls_kv_bootcamp_prd_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_bootcamp_prd_properties_typeProperties_baseUrl')]"
					},
					"ls_Webtable_bootcamp_prd_properties_typeProperties_url": {
						"value": "[parameters('ls_Webtable_bootcamp_prd_properties_typeProperties_url')]"
					},
					"ls_adb_bootcamp_prd_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_adb_bootcamp_prd_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-bootcamp-prd-fbs_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-bootcamp-prd-fbs_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-bootcamp-prd-fbs_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-bootcamp-prd-fbs_ArmTemplate_1')]"
			]
		}
	]
}